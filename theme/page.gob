    <title>Goblin: A Pure CMS</title>

    <!-- Bootstrap core CSS -->
    <link href="theme/css/vendor/bootstrap.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
