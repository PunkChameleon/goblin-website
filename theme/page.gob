<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Goblin: A Pure CMS</title>

    <!-- Bootstrap core CSS -->
    <link href="theme/css/vendor/bootstrap.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="theme/css/carousel.css" rel="stylesheet">

    <!-- Custom styles -->
    <link href="theme/css/styles/style.css" rel="stylesheet">
  </head>
<!-- NAVBAR
================================================== -->
  <body>
    <div class="container">
    <a href="http://opensource.nodejitsu.com/" target="_blank">
      <img
        style="position: absolute; top: 0; right: 0; border: 0"
        src="//www.nodejitsu.com/img/media/foss-r-orange.png"
        alt="Apply for the Nodejitsu free for Open Source program">
    </a>
    <div class="navbar-wrapper">
      <div class="container">

        <div class="navbar navbar-inverse navbar-static-top">
          <div class="container">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand" href="/index.html">{{site_title}}: {{site_description}}</a>
            </div>
            <div class="navbar-collapse collapse">
              <ul class="nav navbar-nav">
                {{#nav}}
                  <li><a href="{{url}}" id="{{id}}">{{item_name}}</a></li>
                {{/nav}}
              </ul>
            </div>
          </div>
        </div>

      </div>
    </div>


    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->

    <div class="container marketing">

      <!-- Three columns of text below the carousel -->
      <div class="row">
        <div class="container">
          <h1>{{page_title}}</h1>
          <p>{{{page_content}}}</p>
        </div>
      </div><!-- /.row -->

      <hr class="featurette-divider">

      <!-- FOOTER -->
      <footer>
        <p class="pull-right"><a href="#">Back to top</a></p>
        <p>Powered by <a href="http://goblin.jit.su/">Goblin</a>. Contribute to <a href="https://github.com/maned/goblin-website" target="_blank"> this website</a>.</p>
      </footer>

    </div><!-- /.container -->
  </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="theme/js/vendor/jquery-2.0.3.min.js"></script>
    <script src="theme/js/vendor/bootstrap.min.js"></script>
    <script src="theme/js/vendor/holder.js"></script>
    <script src="theme/js/kanye.js"></script>
    <script>
      Kanye.yell();
    </script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', '{{ga_id}}', 'jit.su');
      ga('send', 'pageview');

    </script>
  </body>
</html>
